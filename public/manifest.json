{
  "manifest_version": 3,
  "name": "UTEHY Plus",
  "version": "1.0.0",
  "description": "Một tiện ích mở rộng cho trang web sinh viên UTEHY.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "key": "",
  "permissions": ["storage", "cookies"],
  "background": {
    "service_worker": "assets/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://qldaotao.utehy.edu.vn/*"],
      "js": ["assets/content.js"]
    }
  ],

  "host_permissions": ["https://qldaotao.utehy.edu.vn/*"]
}
